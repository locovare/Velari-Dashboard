# Discord OAuth Configuration
# Replace these values with your actual Discord application credentials

# Your Discord Application Client ID
VITE_DISCORD_CLIENT_ID=your_discord_client_id_here

# Your Discord Application Client Secret (keep this secret!)
VITE_DISCORD_CLIENT_SECRET=your_discord_client_secret_here

# Your Discord Bot Client ID (usually the same as CLIENT_ID)
VITE_DISCORD_BOT_CLIENT_ID=your_bot_client_id_here

# OAuth Redirect URI (should match what you set in Discord Developer Portal)
VITE_DISCORD_REDIRECT_URI=http://localhost:3000/callback 